{
  "version": "1.0.1",
  "title": "StructureMap.DynamicInterception",
  "description": "Aspect Oriented Programming Support for StructureMap",
  "language": "en-US",
  "authors": [
    "Dmytro Dziuma, Jeremy D. Miller"
  ],
  "buildOptions": {
    "xmlDoc": true
  },
  "packOptions": {
    "projectUrl": "http://structuremap.github.io",
    "tags": [
      "IoC", "AOP"
    ],
    "licenseUrl": "https://github.com/structuremap/structuremap/raw/master/LICENSE.TXT",
    "repository": {
      "type": "git",
      "url": "git://github.com/structuremap/structuremap"
    }
  },
  "dependencies": {
    "Castle.Core": "4.0.0"
  },
  "frameworks": {
    "net45": {
      "dependencies": {
        "StructureMap": { "target": "project" }
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "StructureMap": { "target": "project" },
        "System.Collections.Concurrent": "4.0.12",
        "System.Reflection.TypeExtensions": "4.1.0",
        "NETStandard.Library": "1.6.0"
      }
    }
  }
}
