{
  "version": "4.4.4",
  "title": "StructureMap",
  "description": "The Original IoC Container for .Net",
  "language": "en-US",
  "authors": [
    "Jeremy D. Miller, Joshua Flanagan, Frank Quednau, Dmytro Dziuma"
  ],
  "packOptions": {
    "projectUrl": "http://structuremap.github.io",
    "tags": [
      "IoC"
    ],
    "licenseUrl": "https://github.com/structuremap/structuremap/raw/master/LICENSE.TXT",
    "repository": {
      "type": "git",
      "url": "git://github.com/structuremap/structuremap"
    }
  },
  "buildOptions": {
    "compile": [
      "**/*.cs",
      "../CommonAssemblyInfo.cs"
    ],
    "xmlDoc": true,
    "nowarn": [ "CS1591" ]
  },
  "frameworks": {
    "net45": {
    },
    "netstandard1.3": {
      "dependencies": {
        "System.Collections.Concurrent": "4.0.12",
        "System.Reflection.TypeExtensions": "4.1.0",
        "NETStandard.Library": "1.6.0"
      }
    },
    "netstandard1.5": {
      "dependencies": {
        "System.Collections.Concurrent": "4.0.12",
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Runtime.Loader": "4.0.0",
        "NETStandard.Library": "1.6.0"
      }
    }
  }
}